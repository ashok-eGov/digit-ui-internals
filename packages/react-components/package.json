{
  "name": "@egovernments/digit-ui-react-components",
  "version": "1.0.17",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "example": "cd example && npm run start",
    "build": "microbundle-crl --no-compress --format modern,cjs",
